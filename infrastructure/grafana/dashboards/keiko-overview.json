{
  "id": null,
  "title": "Keiko Overview",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "editable": true,
  "tags": ["keiko", "overview"],
  "panels": [
    {
      "type": "stat",
      "title": "Health Checks",
      "gridPos": { "x": 0, "y": 0, "w": 8, "h": 6 },
      "targets": [{ "expr": "sum(keiko_health_check_status)", "refId": "A" }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "type": "timeseries",
      "title": "API P95 Latency (s)",
      "gridPos": { "x": 8, "y": 0, "w": 16, "h": 6 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "5xx Error Rate",
      "gridPos": { "x": 0, "y": 6, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "rate(http_requests_total{status=~\"5..\"}[5m]) / rate(http_requests_total[5m])",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Webhook Delivery Success",
      "gridPos": { "x": 12, "y": 6, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "rate(webhook_deliveries_total{status=\"success\"}[5m])",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "KEI-Stream Active Connections",
      "gridPos": { "x": 0, "y": 14, "w": 12, "h": 8 },
      "targets": [
        { "expr": "keiko_websocket_connections_active", "refId": "A" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Webhook Queue Depth",
      "gridPos": { "x": 12, "y": 14, "w": 12, "h": 8 },
      "targets": [{ "expr": "webhook_queue_depth", "refId": "A" }]
    }
  ]
}
