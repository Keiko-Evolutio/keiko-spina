{
  "title": "KEI-RPC Dashboard",
  "panels": [
    {
      "type": "timeseries",
      "title": "gRPC RPS",
      "targets": [{ "expr": "sum by (method)(rate(grpc_requests_total[1m]))" }]
    },
    {
      "type": "timeseries",
      "title": "gRPC Error Rate",
      "targets": [
        {
          "expr": "sum by (method)(rate(grpc_request_errors_total[5m])) / sum by (method)(rate(grpc_requests_total[5m]))"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "gRPC p50",
      "targets": [
        {
          "expr": "histogram_quantile(0.5, sum by (le,method) (rate(grpc_request_duration_seconds_bucket[5m])))"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "gRPC p95",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le,method) (rate(grpc_request_duration_seconds_bucket[5m])))"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "gRPC p99",
      "targets": [
        {
          "expr": "histogram_quantile(0.99, sum by (le,method) (rate(grpc_request_duration_seconds_bucket[5m])))"
        }
      ]
    },
    {
      "type": "table",
      "title": "Top Methods by RPS",
      "targets": [
        { "expr": "topk(10, sum by (method) (rate(grpc_requests_total[5m])))" }
      ]
    }
  ],
  "version": 1
}
