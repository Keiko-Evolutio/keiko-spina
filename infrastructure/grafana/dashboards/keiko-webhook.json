{
  "id": null,
  "title": "Keiko Webhook Delivery",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "editable": true,
  "tags": ["keiko", "webhook"],
  "panels": [
    {
      "type": "timeseries",
      "title": "Deliveries Success/Errors",
      "gridPos": { "x": 0, "y": 0, "w": 24, "h": 8 },
      "targets": [
        {
          "expr": "rate(webhook_deliveries_total{status=\"success\"}[5m])",
          "refId": "A"
        },
        { "expr": "rate(webhook_errors_total[5m])", "refId": "B" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Delivery Duration (s)",
      "gridPos": { "x": 0, "y": 8, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "rate(webhook_delivery_duration_seconds_sum[5m]) / rate(webhook_delivery_duration_seconds_count[5m])",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "DLQ Events",
      "gridPos": { "x": 12, "y": 8, "w": 12, "h": 8 },
      "targets": [{ "expr": "rate(webhook_dlq_total[5m])", "refId": "A" }]
    },
    {
      "type": "timeseries",
      "title": "Active Workers & Queue Depth",
      "gridPos": { "x": 0, "y": 16, "w": 24, "h": 8 },
      "targets": [
        { "expr": "webhook_active_workers", "refId": "A" },
        { "expr": "sum(webhook_queue_depth)", "refId": "B" }
      ]
    }
  ]
}
