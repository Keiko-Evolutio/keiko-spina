{
  "dashboard": {
    "id": null,
    "title": "KEI-MCP API - RED Metrics",
    "tags": ["kei-mcp", "red-metrics", "observability"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "templating": {
      "list": [
        {
          "name": "instance",
          "type": "query",
          "query": "label_values(up{job=\"kei-mcp-api\"}, instance)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*",
          "multi": true
        },
        {
          "name": "endpoint",
          "type": "query",
          "query": "label_values(http_requests_total{job=\"kei-mcp-api\"}, endpoint)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*",
          "multi": true
        },
        {
          "name": "mcp_server",
          "type": "query",
          "query": "label_values(kei_mcp_tool_invocations_total, server_name)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*",
          "multi": true
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "üöÄ Request Rate (Rate)",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{job=\"kei-mcp-api\", instance=~\"$instance\", endpoint=~\"$endpoint\"}[5m])) by (endpoint)",
            "legendFormat": "{{endpoint}}",
            "refId": "A"
          },
          {
            "expr": "sum(rate(http_requests_total{job=\"kei-mcp-api\", instance=~\"$instance\"}[5m]))",
            "legendFormat": "Total RPS",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {"mode": "none", "group": "A"},
              "axisPlacement": "auto",
              "axisLabel": "Requests per Second",
              "scaleDistribution": {"type": "linear"},
              "hideFrom": {"legend": false, "tooltip": false, "vis": false},
              "thresholdsStyle": {"mode": "off"}
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 80}
              ]
            }
          }
        },
        "options": {
          "tooltip": {"mode": "multi", "sort": "desc"},
          "legend": {"displayMode": "list", "placement": "bottom", "calcs": ["lastNotNull", "max"]}
        }
      },
      {
        "id": 2,
        "title": "‚ùå Error Rate (Errors)",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{job=\"kei-mcp-api\", instance=~\"$instance\", endpoint=~\"$endpoint\", status=~\"4..\"}[5m])) by (endpoint) / sum(rate(http_requests_total{job=\"kei-mcp-api\", instance=~\"$instance\", endpoint=~\"$endpoint\"}[5m])) by (endpoint)",
            "legendFormat": "4xx {{endpoint}}",
            "refId": "A"
          },
          {
            "expr": "sum(rate(http_requests_total{job=\"kei-mcp-api\", instance=~\"$instance\", endpoint=~\"$endpoint\", status=~\"5..\"}[5m])) by (endpoint) / sum(rate(http_requests_total{job=\"kei-mcp-api\", instance=~\"$instance\", endpoint=~\"$endpoint\"}[5m])) by (endpoint)",
            "legendFormat": "5xx {{endpoint}}",
            "refId": "B"
          },
          {
            "expr": "sum(rate(http_requests_total{job=\"kei-mcp-api\", instance=~\"$instance\", status=~\"[45]..\"}[5m])) / sum(rate(http_requests_total{job=\"kei-mcp-api\", instance=~\"$instance\"}[5m]))",
            "legendFormat": "Total Error Rate",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 20,
              "gradientMode": "none"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.01},
                {"color": "red", "value": 0.05}
              ]
            }
          }
        },
        "options": {
          "tooltip": {"mode": "multi", "sort": "desc"},
          "legend": {"displayMode": "list", "placement": "bottom", "calcs": ["lastNotNull", "max"]}
        }
      },
      {
        "id": 3,
        "title": "‚è±Ô∏è Response Duration (Duration)",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_seconds_bucket{job=\"kei-mcp-api\", instance=~\"$instance\", endpoint=~\"$endpoint\"}[5m])) by (endpoint, le))",
            "legendFormat": "P50 {{endpoint}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{job=\"kei-mcp-api\", instance=~\"$instance\", endpoint=~\"$endpoint\"}[5m])) by (endpoint, le))",
            "legendFormat": "P95 {{endpoint}}",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket{job=\"kei-mcp-api\", instance=~\"$instance\", endpoint=~\"$endpoint\"}[5m])) by (endpoint, le))",
            "legendFormat": "P99 {{endpoint}}",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "none"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.2},
                {"color": "red", "value": 0.5}
              ]
            }
          }
        },
        "options": {
          "tooltip": {"mode": "multi", "sort": "desc"},
          "legend": {"displayMode": "list", "placement": "bottom", "calcs": ["lastNotNull", "max"]}
        }
      },
      {
        "id": 4,
        "title": "üîß MCP Tool Invocation Rate",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "sum(rate(kei_mcp_tool_invocations_total{server_name=~\"$mcp_server\"}[5m])) by (server_name)",
            "legendFormat": "{{server_name}}",
            "refId": "A"
          },
          {
            "expr": "sum(rate(kei_mcp_tool_invocations_total{server_name=~\"$mcp_server\"}[5m])) by (tool_name)",
            "legendFormat": "Tool: {{tool_name}}",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 10
            }
          }
        }
      },
      {
        "id": 5,
        "title": "‚ùå MCP Tool Error Rate",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "sum(rate(kei_mcp_tool_invocations_total{server_name=~\"$mcp_server\", result=\"failure\"}[5m])) by (server_name) / sum(rate(kei_mcp_tool_invocations_total{server_name=~\"$mcp_server\"}[5m])) by (server_name)",
            "legendFormat": "{{server_name}}",
            "refId": "A"
          },
          {
            "expr": "sum(rate(kei_mcp_tool_validation_failures_total{server_name=~\"$mcp_server\"}[5m])) by (server_name) / sum(rate(kei_mcp_tool_invocations_total{server_name=~\"$mcp_server\"}[5m])) by (server_name)",
            "legendFormat": "Validation Errors {{server_name}}",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 20
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.05},
                {"color": "red", "value": 0.1}
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "title": "‚è±Ô∏è MCP Tool Duration",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(kei_mcp_tool_duration_seconds_bucket{server_name=~\"$mcp_server\"}[5m])) by (server_name, le))",
            "legendFormat": "P50 {{server_name}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(kei_mcp_tool_duration_seconds_bucket{server_name=~\"$mcp_server\"}[5m])) by (server_name, le))",
            "legendFormat": "P95 {{server_name}}",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(kei_mcp_tool_duration_seconds_bucket{server_name=~\"$mcp_server\"}[5m])) by (server_name, le))",
            "legendFormat": "P99 {{server_name}}",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 10
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.2},
                {"color": "red", "value": 0.5}
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "title": "üìä RED Summary Table",
        "type": "table",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 32},
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{job=\"kei-mcp-api\", instance=~\"$instance\"}[5m])) by (endpoint)",
            "legendFormat": "Rate",
            "refId": "A",
            "format": "table",
            "instant": true
          },
          {
            "expr": "sum(rate(http_requests_total{job=\"kei-mcp-api\", instance=~\"$instance\", status=~\"[45]..\"}[5m])) by (endpoint) / sum(rate(http_requests_total{job=\"kei-mcp-api\", instance=~\"$instance\"}[5m])) by (endpoint)",
            "legendFormat": "Error Rate",
            "refId": "B",
            "format": "table",
            "instant": true
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{job=\"kei-mcp-api\", instance=~\"$instance\"}[5m])) by (endpoint, le))",
            "legendFormat": "P95 Duration",
            "refId": "C",
            "format": "table",
            "instant": true
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "displayMode": "list",
              "inspect": false
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Rate"},
              "properties": [
                {"id": "unit", "value": "reqps"},
                {"id": "custom.displayMode", "value": "color-background"},
                {"id": "thresholds", "value": {
                  "steps": [
                    {"color": "red", "value": null},
                    {"color": "yellow", "value": 10},
                    {"color": "green", "value": 100}
                  ]
                }}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "Error Rate"},
              "properties": [
                {"id": "unit", "value": "percentunit"},
                {"id": "custom.displayMode", "value": "color-background"},
                {"id": "thresholds", "value": {
                  "steps": [
                    {"color": "green", "value": null},
                    {"color": "yellow", "value": 0.01},
                    {"color": "red", "value": 0.05}
                  ]
                }}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "P95 Duration"},
              "properties": [
                {"id": "unit", "value": "s"},
                {"id": "custom.displayMode", "value": "color-background"},
                {"id": "thresholds", "value": {
                  "steps": [
                    {"color": "green", "value": null},
                    {"color": "yellow", "value": 0.2},
                    {"color": "red", "value": 0.5}
                  ]
                }}
              ]
            }
          ]
        },
        "options": {
          "showHeader": true,
          "sortBy": [{"desc": true, "displayName": "Rate"}]
        }
      }
    ],
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "prometheus",
          "expr": "changes(up{job=\"kei-mcp-api\"}[1m])",
          "iconColor": "blue",
          "textFormat": "Deployment"
        },
        {
          "name": "High Error Rate",
          "datasource": "prometheus",
          "expr": "ALERTS{alertname=~\".*ErrorRate.*\"}",
          "iconColor": "red",
          "textFormat": "{{alertname}}"
        }
      ]
    }
  }
}
