# =============================================================================
# KEIKO ORCHESTRATOR SERVICE PERFORMANCE CONFIGURATION
# Issue #55: LLM-powered Orchestrator Service with Task Decomposition
# =============================================================================

# Core LLM Orchestrator Performance Targets
KEI_ORCHESTRATOR_ENABLED=true
KEI_ORCHESTRATOR_TASK_ANALYSIS_TIMEOUT_SECONDS=2.0          # <2s Task Analysis (Issue #55)
KEI_ORCHESTRATOR_DECOMPOSITION_TIMEOUT_SECONDS=5.0          # <5s Task Decomposition (Issue #55)
KEI_ORCHESTRATOR_AGENT_SELECTION_TIMEOUT_SECONDS=1.0        # <1s Agent Selection (Issue #55)
KEI_ORCHESTRATOR_PLAN_PERSISTENCE_TIMEOUT_MS=500            # <500ms Plan Persistence (Issue #55)

# Advanced Planning Features Performance
KEI_ORCHESTRATOR_DEPENDENCY_RESOLUTION_TIMEOUT_SECONDS=3.0  # Dependency resolution
KEI_ORCHESTRATOR_PARALLEL_EXECUTION_MAX_WORKERS=10          # Parallel execution workers
KEI_ORCHESTRATOR_PERFORMANCE_PREDICTION_TIMEOUT_MS=200      # ML prediction timeout
KEI_ORCHESTRATOR_COST_OPTIMIZATION_TIMEOUT_MS=100           # Cost optimization timeout

# Saga Pattern Performance
KEI_ORCHESTRATOR_SAGA_EXECUTION_TIMEOUT_SECONDS=30.0        # Saga execution timeout
KEI_ORCHESTRATOR_COMPENSATION_TIMEOUT_SECONDS=10.0          # Compensation timeout
KEI_ORCHESTRATOR_ROLLBACK_TIMEOUT_SECONDS=15.0              # Rollback timeout
KEI_ORCHESTRATOR_STATE_PERSISTENCE_TIMEOUT_MS=200           # State persistence timeout

# Scalability Targets (Issue #55)
KEI_ORCHESTRATOR_CONCURRENT_PLANS=1000                      # 1000+ parallel plans
KEI_ORCHESTRATOR_SUBTASKS_PER_PLAN=100                      # 100+ subtasks per plan
KEI_ORCHESTRATOR_AGENT_EVALUATIONS_PER_SECOND=500           # 500+ agents/second
KEI_ORCHESTRATOR_SAGA_STEPS_MAX=50                          # 50+ steps per saga
KEI_ORCHESTRATOR_MAX_CONCURRENT_ORCHESTRATIONS=20           # Max concurrent orchestrations
KEI_ORCHESTRATOR_MAX_PARALLEL_SUBTASKS=5                    # Max parallel subtasks per orchestration

# Reliability Targets (Issue #55)
KEI_ORCHESTRATOR_PLAN_SUCCESS_RATE=0.95                     # >95% plan success rate
KEI_ORCHESTRATOR_COMPENSATION_SUCCESS_RATE=0.99             # >99% compensation success
KEI_ORCHESTRATOR_SERVICE_AVAILABILITY=0.999                 # 99.9% service availability
KEI_ORCHESTRATOR_STATE_CONSISTENCY=1.0                      # 100% state consistency

# LLM Integration Performance
KEI_ORCHESTRATOR_LLM_MODEL=gpt-4                            # LLM model for analysis
KEI_ORCHESTRATOR_LLM_MAX_TOKENS=2000                        # Max tokens per request
KEI_ORCHESTRATOR_LLM_TEMPERATURE=0.1                        # Low temperature for consistency
KEI_ORCHESTRATOR_LLM_RETRY_ATTEMPTS=3                       # Retry attempts for LLM calls
KEI_ORCHESTRATOR_LLM_RETRY_DELAY_MS=1000                     # Retry delay

# Monitoring and Alerting
KEI_ORCHESTRATOR_METRICS_ENABLED=true                       # Enable metrics collection
KEI_ORCHESTRATOR_HEALTH_CHECK_INTERVAL_SECONDS=30           # Health check interval
KEI_ORCHESTRATOR_ALERT_THRESHOLD_RESPONSE_TIME_MS=1000      # Alert threshold for response time
KEI_ORCHESTRATOR_ALERT_THRESHOLD_ERROR_RATE=0.05            # Alert threshold for error rate

# Caching Configuration
KEI_ORCHESTRATOR_CACHE_ENABLED=true                         # Enable caching
KEI_ORCHESTRATOR_CACHE_TTL_SECONDS=300                      # Cache TTL
KEI_ORCHESTRATOR_CACHE_MAX_SIZE=10000                       # Max cache entries
KEI_ORCHESTRATOR_CACHE_STRATEGY=adaptive                    # Cache strategy

# Resource Limits
KEI_ORCHESTRATOR_MAX_MEMORY_USAGE_MB=2048                   # Max memory usage
KEI_ORCHESTRATOR_MAX_CPU_USAGE_PERCENT=80.0                 # Max CPU usage
KEI_ORCHESTRATOR_MAX_DISK_USAGE_MB=1024                     # Max disk usage

# Security Configuration
KEI_ORCHESTRATOR_ENCRYPTION_ENABLED=true                    # Enable encryption for state
KEI_ORCHESTRATOR_AUDIT_TRAIL_ENABLED=true                   # Enable audit trail
KEI_ORCHESTRATOR_TAMPER_PROTECTION_ENABLED=true             # Enable tamper protection
KEI_ORCHESTRATOR_MULTI_TENANT_ISOLATION=true                # Enable multi-tenant isolation

# Development and Testing
KEI_ORCHESTRATOR_DEBUG_MODE=false                           # Debug mode
KEI_ORCHESTRATOR_PERFORMANCE_TESTING_ENABLED=false          # Performance testing mode
KEI_ORCHESTRATOR_LOAD_TESTING_MAX_REQUESTS=1000             # Max requests for load testing
KEI_ORCHESTRATOR_BENCHMARK_MODE=false                       # Benchmark mode

# Production Optimizations
KEI_ORCHESTRATOR_PRODUCTION_MODE=true                       # Production mode
KEI_ORCHESTRATOR_AGGRESSIVE_OPTIMIZATION=true               # Aggressive optimizations
KEI_ORCHESTRATOR_PREDICTIVE_SCALING=true                    # Predictive scaling
KEI_ORCHESTRATOR_AUTO_TUNING=true                           # Auto-tuning of parameters

# Integration Timeouts
KEI_ORCHESTRATOR_AGENT_REGISTRY_TIMEOUT_SECONDS=2.0         # Agent registry timeout
KEI_ORCHESTRATOR_EVENT_BUS_TIMEOUT_SECONDS=1.0              # Event bus timeout
KEI_ORCHESTRATOR_STATE_STORE_TIMEOUT_SECONDS=3.0            # State store timeout
KEI_ORCHESTRATOR_MONITORING_TIMEOUT_SECONDS=1.0             # Monitoring timeout

# Fallback Configuration
KEI_ORCHESTRATOR_FALLBACK_ENABLED=true                      # Enable fallback mechanisms
KEI_ORCHESTRATOR_FALLBACK_TIMEOUT_SECONDS=10.0              # Fallback timeout
KEI_ORCHESTRATOR_GRACEFUL_DEGRADATION=true                  # Graceful degradation
KEI_ORCHESTRATOR_CIRCUIT_BREAKER_ENABLED=true               # Circuit breaker

# Logging Configuration
KEI_ORCHESTRATOR_LOG_LEVEL=INFO                             # Log level
KEI_ORCHESTRATOR_STRUCTURED_LOGGING=true                    # Structured logging
KEI_ORCHESTRATOR_LOG_PERFORMANCE_METRICS=true               # Log performance metrics
KEI_ORCHESTRATOR_LOG_AUDIT_EVENTS=true                      # Log audit events
